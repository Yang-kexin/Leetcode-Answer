class Solution:
    def numSquares(self, n: int) -> int:
        dp = [1] * (n+1)
        for i in range(1, n+1):
            dp[i] = i
        for i in range(1, int(n**0.5+1)):
            dp[i**2] = 1
            
        for k in range(1, n+1):
            for i in range(1, int(n**0.5+1)):
                if k > i**2:
                    dp[k] = min(dp[k-i**2]+1, dp[k])
                
        return dp[n]
            
